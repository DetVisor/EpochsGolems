<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <ThingDef>
        <defName>DV_Golem_ClayBase</defName>
        <label>golem</label>
        <description>Construct a basic clay golem.</description>
        <descriptionHyperlinks>
            <ThingDef>DV_Golem_Clay</ThingDef>
        </descriptionHyperlinks>
        <thingClass>Building</thingClass>
        <graphicData>
            <texPath>UI/GolemIcons/UI_ClayGolem</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <statBases>
            <WorkToMake>600</WorkToMake>
            <MaxHitPoints>150</MaxHitPoints>
            <Flammability>0.1</Flammability>
        </statBases>
        <comps>
            <li Class="TabulaRasa.CompProperties_PawnSpawner">
                <pawnKind>DV_Golem_Clay</pawnKind>
                <newborn>true</newborn>
            </li>
        </comps>
        <size>(1,1)</size>
        <category>Building</category>
        <pathCost>200</pathCost>
        <passability>Standable</passability>
        <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
        <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
        <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
        <filthLeaving>Filth_RubbleRock</filthLeaving>
        <hasInteractionCell>false</hasInteractionCell>
        <drawerType>MapMeshAndRealTime</drawerType>
        <altitudeLayer>Building</altitudeLayer>
        <repairEffect>Repair</repairEffect>
        <useHitPoints>True</useHitPoints>
        <selectable>true</selectable>
        <surfaceType>Item</surfaceType>
        <tickerType>Normal</tickerType>
    </ThingDef>

    <RecipeDef>
        <defName>DV_Construct_ClayGolem</defName>
        <label>construct clay golem</label>
        <description>Mould a clay golem.</description>
        <jobString>Moulding clay golem.</jobString>
        <workSpeedStat>DV_GolemFormingSpeed</workSpeedStat>
        <effectWorking>ClayTurning</effectWorking>
        <soundWorking>Recipe_MakeStoneBlocks</soundWorking>
        <workAmount>1500</workAmount>
        <unfinishedThingDef>DV_UnfinishedGolem</unfinishedThingDef>
        <descriptionHyperlinks>
            <ThingDef>DV_Golem_Clay</ThingDef>
        </descriptionHyperlinks>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>DV_Clay</li>
                    </thingDefs>
                </filter>
                <count>75</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>DV_Exotic_AnimaDoll</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>DV_Clay</li>
                <li>DV_Exotic_AnimaDoll</li>
            </thingDefs>
        </fixedIngredientFilter>
        <recipeUsers>
            <li>DV_GolemancySpot</li>
        </recipeUsers>
        <products>
            <DV_Golem_ClayBase>1</DV_Golem_ClayBase>
        </products>
        <skillRequirements>
            <Crafting>6</Crafting>
        </skillRequirements>
        <workSkill>Crafting</workSkill>
        <researchPrerequisite>DV_GolemResearch_BasicGolemancy</researchPrerequisite>
    </RecipeDef>


